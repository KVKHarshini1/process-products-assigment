<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Asynchronous Timer with Callback</title>
</head>
<body>

<script>
    // Step 1: Define the timer function
    function timer(duration, onComplete) {
        setTimeout(() => {
            onComplete(`Timer of ${duration} ms finished`);
        }, duration);
    }

    // Step 2: Calling function with callback
    timer(3000, function(message) {
        console.log(message);
    });
</script>

</body>
</html>
